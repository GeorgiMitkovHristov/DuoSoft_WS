				<xs:restriction base="xs:token">
					<xs:enumeration value="Individual"/>
					<xs:enumeration value="Collective"/>
					<xs:enumeration value="Certificate"/>
					<xs:enumeration value="Guarantee"/>
					<xs:enumeration value="Defensive"/>

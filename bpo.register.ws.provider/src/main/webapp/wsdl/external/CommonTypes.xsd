			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<!-- ====================================================================== -->
	<!-- Filename    : GoodsServicesType.xsd                                    -->
	<!-- Author      : TM-XML WG                                                -->
	<!-- Version     : 1.3                                                      -->
	<!-- Last Update : 2009-10-23                                               -->

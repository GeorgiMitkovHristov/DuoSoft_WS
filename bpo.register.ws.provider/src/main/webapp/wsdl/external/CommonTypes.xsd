	</xs:complexType>
	<!-- ====================================================================== -->
	<!-- Filename    : InventorType.xsd                                        -->
	<!-- ====================================================================== -->
	<xs:complexType name="InventorType">
		<xs:sequence>
			<xs:element name="InventorNationalityCode" type="xs:string"/>
